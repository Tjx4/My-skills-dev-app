<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto">
    <data>
        <variable
                name="housesViewModel"
                type="co.za.dvt.myskilldevapp.features.houses.HousesViewModel"/>
    </data>

    <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            tools:context=".features.houses.HousesActivity">


        <Button
                style="@style/MySkillsButton"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="Get houses"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                android:layout_marginTop="80dp"
                android:onClick="onGetHousesbuttonClicked"/>

    </androidx.constraintlayout.widget.ConstraintLayout>
</layout>
